                <Label Content="户型图收藏" HorizontalAlignment="Left" Margin="16,1,0,0" VerticalAlignment="Top" FontSize="14"/>
                <Grid HorizontalAlignment="Left" Height="1" Margin="10,30,0,0" VerticalAlignment="Top" Width="614" Background="{DynamicResource {x:Static SystemColors.WindowTextBrushKey}}"/>
