                <ComboBox HorizontalAlignment="Left" Height="20" Margin="89,60,0,0" VerticalAlignment="Top" Width="79"/>
                <ComboBox HorizontalAlignment="Left" Height="20" Margin="480,60,0,0" VerticalAlignment="Top" Width="79"/>
                <ComboBox HorizontalAlignment="Left" Height="20" Margin="265,60,0,0" VerticalAlignment="Top" Width="79" RenderTransformOrigin="1.405,1"/>
                <ComboBox HorizontalAlignment="Left" Height="20" Margin="89,98,0,0" VerticalAlignment="Top" Width="79" RenderTransformOrigin="2.709,2.9"/>
                <Label Content="市" HorizontalAlignment="Left" Margin="209,57,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.025,0.2" FontSize="14"/>
                <TextBox HorizontalAlignment="Left" Height="23" Margin="480,94,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
                <TextBox HorizontalAlignment="Left" Height="23" Margin="265,94,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
            </Grid>
        </ScrollViewer>

    </Grid>
</Window>
